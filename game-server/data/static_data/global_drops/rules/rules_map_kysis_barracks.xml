<?xml version="1.0" encoding="utf-8"?>
<global_rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../global_rules.xsd">
	<!-- Multiple Monster Drops -->
	<gd_rule rule_name="Named Key" base_chance="100" no_reduction="true">
		<gd_maps>
			<gd_map map_id="301280000" name="Kysis_Barracks"/>
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233670" name="Treasurer Darmaraja"/>
			<gd_npc npc_id="233671" name="Treasurer Swaraja"/>
			<gd_npc npc_id="233672" name="Treasurer Chandra"/>
			<gd_npc npc_id="233673" name="Treasurer Dragagh"/>
		</gd_npcs>
		<gd_items>
			<gd_item id="185000061" name="Kysis Armory Key"/>
			<gd_item id="185000062" name="Kysis Supply Base Key"/>
			<gd_item id="185000063" name="Kysis Operations Room Key"/>
		</gd_items>
	</gd_rule>
	<!-- Single Monster Drops -->
	<gd_rule rule_name="Named Treasure Key" base_chance="100" no_reduction="true" each_member="true">
		<gd_maps>
			<gd_map map_id="301280000" name="Kysis_Barracks"/>
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233676" name="Enraged Kysis Duke"/>
		</gd_npcs>
		<gd_items>
			<gd_item id="185000064" name="Kysis Treasure Room Key"/>
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Gold Key" base_chance="100" no_reduction="true" each_member="true">
		<gd_maps>
			<gd_map map_id="301280000" name="Kysis_Barracks"/>
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233676" name="Enraged Kysis Duke"/>
		</gd_npcs>
		<gd_items>
			<gd_item id="185000065" name="Kysis Gold Room Key"/>
		</gd_items>
	</gd_rule>
</global_rules>