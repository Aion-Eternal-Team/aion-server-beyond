<?xml version="1.0" encoding="utf-8"?>
<global_rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../global_rules.xsd">
	<!-- Multiple Monster Drops -->
	<gd_rule rule_name="Named Key" base_chance="100" no_reduction="true">
		<gd_maps>
			<gd_map map_id="300130000" name="Miren_Chamber" />
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="215216" name="Treasurer Karpasha" />
			<gd_npc npc_id="215217" name="Treasurer Pasikar" />
			<gd_npc npc_id="215218" name="Treasurer Subatita" />
			<gd_npc npc_id="215219" name="Treasurer Mahirwa" />
		</gd_npcs>
		<gd_items>
			<gd_item id="185000066" name="Miren Armory Key" />
			<gd_item id="185000067" name="Miren Supply Base Key" />
			<gd_item id="185000068" name="Miren Operations Room Key" />
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Treasure Key" base_chance="100" no_reduction="true" member_limit="6">
		<gd_maps>
			<gd_map map_id="300130000" name="Miren_Chamber" />
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="215221" name="Weakened Miren Prince" />
			<gd_npc npc_id="215222" name="Awakened Miren Prince" />
		</gd_npcs>
		<gd_items>
			<gd_item id="185000069" name="Miren Treasure Room Key" />
		</gd_items>
	</gd_rule>
	<!-- Single Monster Drops -->
	<gd_rule rule_name="Named Gold Key" base_chance="100" no_reduction="true" member_limit="6">
		<gd_maps>
			<gd_map map_id="300130000" name="Miren_Chamber" />
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="215222" name="Awakened Miren Prince" />
		</gd_npcs>
		<gd_items>
			<gd_item id="185000070" name="Miren Gold Room Key" />
		</gd_items>
	</gd_rule>
</global_rules>